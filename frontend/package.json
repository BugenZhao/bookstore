{
  "name": "bookstore-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@devexpress/dx-react-core": "2.7.5",
    "@devexpress/dx-react-grid": "2.7.5",
    "@devexpress/dx-react-grid-bootstrap4": "2.7.5",
    "bootstrap": "^5.0.0-beta2",
    "classnames": "^2.3.1",
    "echarts-for-react": "^3.0.1",
    "immer": "^9.0.1",
    "moment": "^2.29.1",
    "popper.js": "^1.16.1",
    "react": "^17.0.1",
    "react-awesome-reveal": "3.4.0",
    "react-bootstrap": "^2.0.0-alpha.0",
    "react-bootstrap-icons": "^1.4.0",
    "react-datepicker": "^3.8.0",
    "react-dom": "^17.0.1",
    "react-hook-form": "^7.0.0",
    "react-router-dom": "^5.2.0",
    "swr": "^0.5.5",
    "titleize": "^2.1.0",
    "use-immer": "^0.5.1",
    "use-local-storage": "^2.1.1"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "varsIgnorePattern": "^_",
          "argsIgnorePattern": "^_"
        }
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-datepicker": "^3.1.8",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "2.2.1",
    "typescript": "^4.2.3",
    "vite": "^2.3.7",
    "vite-react-jsx": "^1.1.1"
  }
}
